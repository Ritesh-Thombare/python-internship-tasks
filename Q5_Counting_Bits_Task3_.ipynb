        "Q5 Counting Bits"
      ],
      "metadata": {
        "id": "3w6zNLIJi-jj"
      }
    },
    {
      "cell_type": "code",
      "execution_count": 1,
      "metadata": {
        "id": "cEK4ceLaiygz"
      },
      "outputs": [],
      "source": [
        "class Solution:\n",
        "    def countBits(self, n: int):\n",
        "        res = []\n",
        "\n",
        "        for i in range(n + 1):\n",
        "            ones = 0\n",
        "            x = i\n",
        "\n",
        "            while x > 0:\n",
        "                ones += x % 2\n",
        "                x //= 2\n",
        "\n",
        "            res.append(ones)\n",
        "\n",
        "        return res"
      ]
    }
  ]
}