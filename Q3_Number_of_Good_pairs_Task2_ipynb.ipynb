        "Q3 Number of Good pairs"
      ],
      "metadata": {
        "id": "o7uNs7pM6ewH"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "Oh1f_gMt5Yra"
      },
      "outputs": [],
      "source": [
        "class Solution:\n",
        "    def numIdenticalPairs(self, nums):\n",
        "        count = 0\n",
        "\n",
        "        for i in range(len(nums)):\n",
        "            for j in range(i + 1, len(nums)):\n",
        "                if nums[i] == nums[j]:\n",
        "                    count += 1\n",
        "\n",
        "        return count\n"
      ]
    }
  ]
}